<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="FristFont">
        <Setter Property="TextElement.FontFamily" Value="/Resources/font/#Adobe 黑体 Std" />
    </Style>
    <Style x:Key="SecondFont">
        <Setter Property="TextElement.FontFamily" Value="/Resources/font/#阿里巴巴普惠体 R" />
    </Style>
    <Style x:Key="ThirdFont">
        <Setter Property="TextElement.FontFamily" Value="/Resources/font/#方正粗黑宋简体" />
    </Style>
    <Style x:Key="FourFont">
        <Setter Property="TextElement.FontFamily" Value="/Resources/font/#DS-DIGIT" />
    </Style>
    <Style x:Key="FiveFont">
        <Setter Property="TextElement.FontFamily" Value="/Resources/font/#造字工房典黑粗体" />
    </Style>
</ResourceDictionary>